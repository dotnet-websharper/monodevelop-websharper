<?xml version="1.0"?>
<Template Originator="IntelliFactory" Created="7/29/2015" LastModified="7/29/2015">
	<TemplateConfiguration>
		<_Name>WebSharper Client-Server UI.Next Application</_Name>
		<_Description>A website defined in C# (client and server), served with WebSharper sitelets and using UI.Next</_Description>
		<_Category>Zafir</_Category>
		<LanguageName>C#</LanguageName>
	</TemplateConfiguration>
	<Combine name="${ProjectName}" directory=".">
		<Options>
			<StartupProject>${ProjectName}</StartupProject>
		</Options>
		<Project name="${ProjectName}" directory="." type="CSharpSiteletsUINextProjectBinding">
		  <Packages>
		    <Package id="Zafir" />
        <Package id="Zafir.CSharp" />
        <Package id="Zafir.UI.Next" />
		  </Packages>
		</Project>
	</Combine>
</Template>
