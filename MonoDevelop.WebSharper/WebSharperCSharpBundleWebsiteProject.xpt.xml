<?xml version="1.0"?>
<Template Originator="IntelliFactory" Created="4/4/2014" LastModified="2/25/2015">
	<TemplateConfiguration>
		<_Name>WebSharper Single-Page Application</_Name>
		<_Description>An empty single-page HTML application</_Description>
		<_Category>Zafir</_Category>
		<LanguageName>C#</LanguageName>
	</TemplateConfiguration>
	<Combine name="${ProjectName}" directory=".">
		<Options>
			<StartupProject>${ProjectName}</StartupProject>
		</Options>
		<Project name="${ProjectName}" directory="." type="CSharpBundleWebsiteProjectBinding">
		  <Packages>
		    <Package id="Zafir" />
        <Package id="Zafir.CSharp" />
        <Package id="Zafir.Html" />
		  </Packages>
		</Project>
	</Combine>
</Template>
