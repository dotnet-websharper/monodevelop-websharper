<?xml version="1.0"?>
<Template Originator="IntelliFactory" Created="4/4/2014" LastModified="2/25/2015">
	<TemplateConfiguration>
		<_Name>WebSharper Extension</_Name>
		<_Description>A WebSharper extension to existing JavaScript code with the WebSharper Interface Generator (WIG) tool</_Description>
		<_Category>WebSharper</_Category>
		<LanguageName>F#</LanguageName>
	</TemplateConfiguration>
	<Combine name="${ProjectName}" directory=".">
		<Options>
			<StartupProject>${ProjectName}</StartupProject>
		</Options>
		<Project name="${ProjectName}" directory="." type="ExtensionProjectBinding">
		  <Packages>
		    <Package id="WebSharper" />
		  </Packages>
		</Project>
	</Combine>
</Template>
