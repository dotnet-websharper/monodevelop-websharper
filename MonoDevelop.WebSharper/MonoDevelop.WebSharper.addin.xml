<?xml version="1.0"?>
<Addin id="Zafir.CSharp" namespace="MonoDevelop" name="WebSharper 4 for C# (Zafir)" author="IntelliFactory" copyright="Copyright (c) 2008-2016 IntelliFactory" description="Provides WebSharper 4 (Zafir) support for MonoDevelop and Xamarin Studio" category="Web Development" version="4.0.55.0-alpha">
	<Dependencies>
		<Addin id="::MonoDevelop.Core" version="5.0" />
		<Addin id="::MonoDevelop.Ide" version="5.0" />
	</Dependencies>
	<Runtime>
		<Import assembly="WebSharper.Templates.dll" />
		<Import assembly="MonoDevelop.WebSharper.dll" />
		<Import assembly="FSharp.Core.dll" />
		<Import assembly="SharpCompress.dll" />
	</Runtime>
	<Extension path="/MonoDevelop/Ide/ProjectTemplates">
		<ProjectTemplate id="WebSharperCSharpSiteletsUINextProject" resource="WebSharperCSharpSiteletsUINextProject.xpt.xml" />
		<ProjectTemplate id="WebSharperCSharpBundleWebsiteProject" resource="WebSharperCSharpBundleWebsiteProject.xpt.xml" />
		<ProjectTemplate id="WebSharperCSharpBundleUINextProject" resource="WebSharperCSharpBundleUINextProject.xpt.xml" />
		<ProjectTemplate id="WebSharperCSharpLibraryProject" resource="WebSharperCSharpLibraryProject.xpt.xml" />
	</Extension>
	<Extension path="/MonoDevelop/ProjectModel/ProjectBindings">
		<ProjectBinding id="CSharpSiteletsUINextProjectBinding" class="MonoDevelop.WebSharper.CSharpSiteletsUINextProjectBinding" />
		<ProjectBinding id="CSharpBundleWebsiteProjectBinding" class="MonoDevelop.WebSharper.CSharpBundleWebsiteProjectBinding" />
		<ProjectBinding id="CSharpBundleUINextProjectBinding" class="MonoDevelop.WebSharper.CSharpBundleUINextProjectBinding" />
		<ProjectBinding id="CSharpLibraryProjectBinding" class="MonoDevelop.WebSharper.CSharpLibraryProjectBinding" />
	</Extension>
</Addin>
