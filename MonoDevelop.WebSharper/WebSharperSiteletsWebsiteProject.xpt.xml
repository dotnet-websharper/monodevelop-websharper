<?xml version="1.0"?>
<Template Originator="IntelliFactory" Created="4/4/2014" LastModified="4/4/2014">
	<TemplateConfiguration>
		<_Name>WebSharper Sitelets Website</_Name>
		<_Description>A website defined in F# (client and server), served with WebSharper sitelets</_Description>
		<_Category>WebSharper</_Category>
		<LanguageName>F#</LanguageName>
	</TemplateConfiguration>
	<Combine name="${ProjectName}" directory=".">
		<Options>
			<StartupProject>${ProjectName}</StartupProject>
		</Options>
		<Project name="${ProjectName}" directory="." type="SiteletsWebsiteProjectBinding" />
	</Combine>
</Template>
